<Window
    x:Class="Dynamo.Nodes.PresetOverwritePrompt"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:p="clr-namespace:Dynamo.Wpf.Properties"
    xmlns:ui="clr-namespace:Dynamo.UI"
    Title="{x:Static p:Resources.PresetsWindowTitle}"
    Width="372"
    Height="192.226"
    Style="{DynamicResource DynamoWindowStyle}"
    WindowStartupLocation="CenterOwner">

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dynamo.Wpf;component/Themes/Modern/Generic.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid Background="#333">

        <StackPanel Name="Panel" Margin="15">

            <TextBlock
                Name="Indicator"
                Padding="0,15,0,15"
                FontSize="15"
                Foreground="WhiteSmoke" />
            <StackPanel
                Margin="0,25,0.2,0"
                HorizontalAlignment="Center"
                Orientation="Horizontal"
                RenderTransformOrigin="0.031,1.281">
                <Button
                    x:Name="okButton"
                    Width="75"
                    Margin="5"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Click="OK_Click"
                    Content="{x:Static p:Resources.OKButton}"
                    IsDefault="True"
                    Style="{DynamicResource ResourceKey=STextButton}"
                    TabIndex="2" />
                <Button
                    x:Name="cancelButton"
                    Width="75"
                    Margin="5"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Click="Cancel_Click"
                    Content="{x:Static p:Resources.CancelButton}"
                    IsCancel="True"
                    Style="{DynamicResource ResourceKey=STextButton}"
                    TabIndex="3" />

            </StackPanel>

        </StackPanel>
    </Grid>
</Window>